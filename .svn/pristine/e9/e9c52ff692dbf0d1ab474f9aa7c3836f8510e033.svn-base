.coupon_box {
    margin-left: 30rpx;
    margin-right: 30rpx;
    width: 702rpx;
    box-shadow: 0 0 16px 0 rgba(0, 0, 0, 0.20);
    height:198rpx;
}

.coupon_left {
    background: #d7b775;
    position: relative;
    color: #fff;
    padding-left: 14rpx;
    padding-top: 55rpx;
    padding-bottom: 12rpx;
}

.coupon_right {
    background: #fff;
    padding-left: 23rpx;
    padding-right: 26rpx;
    padding-top: 45rpx;
    padding-bottom: 23rpx;
    width: 390rpx;
}


/* 按钮 */
.counpon-addBtn {
    margin-left: 50rpx;
    margin-right: 50rpx;
    margin-top: 150rpx;
    margin-bottom: 100rpx;
    background: #1A79FF;
    color: white;
    border-radius: 20rpx;
}


.coupon_center {
    width: 26rpx;
    height: 100%;
    position: absolute;
    right: -13rpx;
    top: 0;
}

.half_circle {
    width: 26rpx;
    height: 13rpx;
    border-radius: 0 0 13rpx 13rpx;
    background: #D8D8D8;
}

.half_circle2 {
    width: 26rpx;
    height: 13rpx;
    border-radius: 13rpx 13rpx 0 0;
    background: #D8D8D8;
}

.whole_circle {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background: #f5f5f9;
}

.over_two_ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}



.coupon-list .coupon-item {
    margin-top: 20rpx;
    position: relative;
    width: 100vw;
    /* border-bottom: 2rpx solid #E9E9E9; */
    box-sizing: border-box;
    background: #fff;
    z-index: 999;
}

.slide-coupon-list .slide-coupon-item {
    position: relative;
    width: 100vw;
    box-sizing: border-box;
    background: #fff;
    z-index: 999;
}

.coupon-list .coupon-item movable-area {
    height: 198rpx;
    width: calc(100vw - 120rpx);
}

.coupon-list .coupon-item movable-view {
    height: 198rpx;
    width: 100vw;
    background: #fff;
    z-index: 999;
}

.coupon-list .coupon-item .delete-btn {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    width: 120rpx;
    font-family: PingFangSC-Regular;
    font-size: 38rpx;
    color: #FFFFFF;
    line-height: 198rpx;
    z-index: 1;
    background: #E51831 ;
    text-align: center;
}

.coupon-list .coupon-item-wrap {
    position: relative;
    display: flex;
    align-items: center;
    padding: 8rpx 0 20rpx 20rpx;
    box-sizing: border-box;
}

.coupon-list .coupon-item-wrap .coupon-movable-item {
    flex: 1;
    overflow: hidden;
}

.coupon-list .coupon-item-wrap .coupon-movable-item-name {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #71747A;
    line-height: 60rpx;
    margin-right: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.coupon-list .coupon-item-wrap .coupon-movable-item-code {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #969AA3;
}

.coupon-list .coupon-item-wrap .coupon-movable-item-amount {
    flex: 0 0 auto;
    padding-right: 80rpx;
    position: relative;
}

.coupon-list .coupon-item-wrap .coupon-movable-item-amount .amount {
    width: 120rpx;
    font-size: 28rpx;
    color: #383A3D;
    line-height: 60rpx;
}

.coupon-list .coupon-item-wrap .coupon-movable-item-amount .unit {
    position: absolute;
    top: 0;
    right: 30rpx;
    font-size: 28rpx;
    color: #969AA3;
    line-height: 60rpx;
}