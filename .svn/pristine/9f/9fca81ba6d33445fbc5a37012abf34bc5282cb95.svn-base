/* 头部背景view */
.coupon-headerView {
    height: 416rpx;
    width: 100%;
}

.coupon-headerImg {
    width: 100%;
    height: 416rpx;
}

.coupon_box {
    margin-left:30rpx;
    margin-right:30rpx;
    position: absolute;
    width: 702rpx;
    top: 60rpx;
    height:198rpx;
}

.coupon_left {
    background: #d7b775;
    position: relative;
    color: #fff;
    padding-left: 14rpx;
    padding-top: 55rpx;
    padding-bottom: 12rpx;
}

.coupon_right {
    background: #fff;
    padding-left: 23rpx;
    padding-right: 26rpx;
    padding-top: 42rpx;
    padding-bottom: 23rpx;
    width: 390rpx;

}

/* 页面信息展示 */
.counpon-allView{
    position: absolute;
    right:25rpx;
    top: 281rpx;
    left:25rpx;
    background: white;
    box-shadow: 0 0 16px 0 rgba(0,0,0,0.20);
    border-radius: 16px;
  
  }
  
  .counpon-detailView{
      margin-top: 30rpx;
      margin-bottom: 40rpx;
      width: 700rpx;
  }
  
  /* 手机号码验证码提示 */
  .counpon-detailInfo{
    position: relative;
    font-size: 28rpx;
    padding-left: 60rpx;
    padding-top: 20rpx;
    display: block;
    color: #666666 ;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;

  }
  
  
  
  /* 按钮 */
  .counpon-confirmBtn{
    margin-left: 50rpx;
    margin-right: 50rpx;
    margin-top: 500rpx;
    background: #1A79FF ;
    color: white;
    border-radius: 20rpx;
  }


.coupon_center {
    width: 26rpx;
    height: 100%;
    position: absolute;
    right: -13rpx;
    top: 0;
}

.half_circle {
    width: 26rpx;
    height: 13rpx;
    border-radius: 0 0 13rpx 13rpx;
    background: #D8D8D8;
}

.half_circle2 {
    width: 26rpx;
    height: 13rpx;
    border-radius: 13rpx 13rpx 0 0;
    background: #D8D8D8;
}

.whole_circle {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background: #f5f5f9;
}

.over_two_ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}